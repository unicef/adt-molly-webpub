  "txt_p10_t0_easy_read": "\u4f1a\u8bae\u7ed3\u675f\u540e\uff0c\u83ab\u8389\u671b\u5411\u7a97\u5916\u3002\u201c\u7cdf\u4e86\uff0c\u201d\u5979\u8bf4\uff0c\u201c\u4e0b\u96e8\u4e86\u3002\u73b0\u5728\u6211\u4eec\u4e0d\u80fd\u5230\u5916\u9762\u53bb\u7ec3\u4e60\u4e86\u3002\u201d\u4f46\u83ab\u8389\u5bf9\u8fd9\u573a\u96e8\u5374\u611f\u5230\u975e\u5e38\u9ad8\u5174\u3002\u5979\u4e3a\u4ec0\u4e48\u4f1a\u9ad8\u5174\u5462\uff1f",
  "txt_p10_t1": "9",
  "txt_p10_t1_easy_read": "\u4e5d\u3002",
  "img_p5_r0": "\u4e24\u540d\u8eab\u7a7f\u84dd\u8272\u7403\u8863\u548c\u7ea2\u8272\u77ed\u88e4\u7684\u6606\u866b\u8fd0\u52a8\u5458\u817e\u8dc3\uff1a\u4e00\u540d\u6cbf\u7740\u8349\u5730\u8dd1\u9053\u8fdb\u884c\u8df3\u8fdc\uff0c\u53e6\u4e00\u540d\u8d8a\u8fc7\u8df3\u9ad8\u6a2a\u6746\uff0c\u8dc3\u5411\u7ea2\u8272\u843d\u57ab\u3002",
  "img_p6_r0": "\u5728\u4e00\u7247\u8349\u5730\u4e0a\uff0c\u4e00\u4e2a\u6606\u866b\u5c0f\u5b69\u671d\u7740\u6c99\u5751\u8df3\u8fdc\uff0c\u53e6\u4e00\u4e2a\u7a7f\u7740\u62a4\u5177\u3001\u624b\u6301\u677f\u7403\u62cd\uff0c\u7ad9\u5728\u4e09\u67f1\u95e8\u65c1\uff1b\u5728\u4e0b\u65b9\uff0c\u4e00\u4e2a\u7a7f\u7740\u540c\u6837\u88c5\u5907\u7684\u7eff\u8272\u5973\u5b69\u5728\u7d2b\u8272\u7684\u9634\u5f71\u4e2d\u8e72\u7740\uff0c\u9762\u9732\u5fe7\u8272\u3002",
  "img_p7_r0": "\u56db\u540d\u7a7f\u84dd\u8272\u4e0a\u8863\u3001\u7ea2\u8272\u77ed\u88e4\u7684\u6606\u866b\u961f\u53cb\u53cc\u624b\u53c9\u8170\u7ad9\u7acb\uff0c\u5728\u843d\u53f6\u6563\u5e03\u7684\u8349\u5730\u4e0a\uff0c\u4e0d\u5b89\u5730\u671b\u5411\u53f3\u4fa7\u7684\u4e00\u4e2a\u601d\u8003\u6c14\u6ce1\u3002",
  "img_p8_r0": "\u5728\u6cbc\u6cfd\u91cc\u7684\u4e00\u5ea7\u539f\u6728\u821e\u53f0\u4e0a\uff0c\u4e00\u53ea\u7a7f\u7740\u9ec4\u8272\u8fde\u8863\u88d9\u7684\u86b1\u8722\u9ad8\u4e3e\u7740\u4e00\u5ea7\u91d1\u8272\u5956\u676f\uff1b\u4e0e\u6b64\u540c\u65f6\uff0c\u4e09\u53ea\u8eab\u7a7f\u7d2b\u8272\u6bdb\u8863\u7684\u9752\u86d9\u5728\u4e00\u65c1\u89c2\u770b\u3002\u5728\u9ad8\u9ad8\u7684\u53f6\u4e1b\u4e4b\u95f4\uff0c\u4e00\u5f20\u7cfb\u7740\u7f0e\u5e26\u7684\u7eff\u8272\u684c\u5b50\u4e0a\u8fd8\u6446\u7740\u53e6\u5916\u4e24\u5ea7\u5956\u676f\uff0c\u6697\u793a\u7740\u4e00\u573a\u9881\u5956\u4eea\u5f0f\u3002",
  "img_p9_r0": "\u5728\u4e00\u4e2a\u6e29\u99a8\u7684\u5ba2\u5385\u91cc\uff0c\u4e00\u4f4d\u7a7f\u7740\u7d2b\u8272\u8fde\u8863\u88d9\u7684\u6606\u866b\u6bcd\u4eb2\u7ad9\u5728\u95e8\u53e3\uff0c\u624b\u91cc\u62ff\u7740\u4e00\u4e2a\u5305\uff0c\u6b63\u5bf9\u4e24\u4e2a\u7a7f\u7740\u84dd\u8272\u4e0a\u8863\u548c\u7ea2\u8272\u77ed\u88e4\u3001\u61d2\u6563\u5730\u9760\u5728\u6905\u5b50\u4e0a\u7684\u5b69\u5b50\u8bf4\u8bdd\uff0c\u800c\u5899\u4e0a\u6302\u7740\u88c5\u6846\u7684\u5bb6\u5ead\u8096\u50cf\u3002",
  "img_p10_r0": "\u5728\u4e00\u95f4\u7834\u65e7\u7684\u623f\u95f4\u91cc\uff0c\u96e8\u6ef4\u987a\u7740\u4e00\u6247\u62f1\u5f62\u7a97\u6ed1\u843d\uff0c\u4e00\u53ea\u7eff\u8272\u7684\u866b\u5b50\u5411\u5916\u5f20\u671b\uff0c\u4e00\u53ea\u68d5\u8272\u7684\u866b\u5b50\u762b\u5750\u5728\u6905\u5b50\u4e0a\uff0c\u8fd8\u6709\u4e00\u53ea\u5c0f\u5c0f\u7684\u84dd\u8272\u866b\u5b50\u4ece\u684c\u5b50\u5e95\u4e0b\u63a2\u51fa\u5934\u6765\u3002",
  "sec_p5_s0_eli5": "\u50cf\u62115\u5c81\u4e00\u6837\u7684\u89e3\u91ca\uff1a\n- \u4e09\u4e2a\u670b\u53cb\u2014\u2014\u52d2\u7f57\u4f0a\u3001\u83b1\u5c3c\u548c\u5927\u536b\u2014\u2014\u662f\u4ed6\u4eec\u961f\u4f0d\u91cc\u6700\u4f1a\u8df3\u7684\u4eba\uff0c\u4ed6\u4eec\u7684\u961f\u4f0d\u53eb\u201c\u8df3\u8dc3\u8005\u4ff1\u4e50\u90e8\u201d\u3002\u53ef\u4ee5\u628a\u4ed6\u4eec\u60f3\u8c61\u6210\u4ff1\u4e50\u90e8\u91cc\u7684\u8d85\u7ea7\u82f1\u96c4\u7ea7\u8df3\u8dc3\u8005\u3002\n- \u56fe\u7247\u91cc\uff0c\u4e00\u4f4d\u8df3\u8dc3\u8005\u55d6\u5730\u5411\u524d\u51b2\uff0c\u98de\u8fc7\u4e00\u6839\u9ad8\u9ad8\u7684\u6a2a\u6746\uff0c\u7136\u540e\u5b89\u5168\u5730\u843d\u5728\u67d4\u8f6f\u7684\u7ea2\u8272\u57ab\u5b50\u4e0a\u3002\u4ed6\u4eec\u7684\u8df3\u8dc3\u5c31\u662f\u8fd9\u4e48\u5389\u5bb3\u3002\n- \u4f46\u662f\u52d2\u7f57\u4f0a\u5468\u4e94\u6709\u522b\u7684\u5b89\u6392\uff1a\u4ed6\u8981\u53bb\u6253\u677f\u7403\uff0c\u8fd9\u662f\u4e00\u79cd\u7528\u7403\u68d2\u548c\u7403\u8fdb\u884c\u7684\u8fd0\u52a8\u3002\u6240\u4ee5\u5468\u4e94\u4ed6\u4e0d\u80fd\u53c2\u52a0\u8df3\u8dc3\u8005\u4ff1\u4e50\u90e8\u7684\u8df3\u8dc3\u6d3b\u52a8\uff0c\u56e0\u4e3a\u4ed6\u8981\u53bb\u53c2\u52a0\u4ed6\u7684\u677f\u7403\u6bd4\u8d5b\u3002",
  "sec_p6_s0_eli5": "- \u8c01\uff1a\u83ab\u8389\u00b7\u970d\u73c0\u3002\n- \u505a\u4ec0\u4e48\uff1a\u5979\u5fc5\u987b\u9876\u66ff\u52d2\u7f57\u4f0a\u7684\u4f4d\u7f6e\uff0c\u5c31\u50cf\u5f53\u4f60\u7684\u670b\u53cb\u4e0d\u80fd\u4e0a\u573a\u65f6\u4f60\u5fc5\u987b\u52a0\u5165\u7403\u961f\u4e00\u6837\u3002\n- \u95ee\u9898\uff1a\u83ab\u8389\u4e0d\u592a\u64c5\u957f\u8df3\u8dc3\u3002\n- \u611f\u53d7\uff1a\u5979\u4e00\u76f4\u5bb3\u6015\u53bb\u505a\u8fd9\u4ef6\u4e8b\u3002\n- \u56fe\u7247\u6240\u793a\uff1a\u4e00\u53ea\u5c0f\u866b\u5728\u8df3\u8dc3\uff0c\u53e6\u4e00\u4e2a\u62ff\u7740\u7403\u68d2\u51c6\u5907\u6bd4\u8d5b\uff0c\u800c\u83ab\u8389\u9762\u5e26\u5fe7\u8651\u62b1\u819d\u800c\u5750\u2014\u2014\u5979\u5728\u60f3\u8c61\u90a3\u6b21\u5927\u8df3\u8dc3\uff0c\u611f\u5230\u5bb3\u6015\u3002\n- \u5927\u610f\uff1a\u6709\u65f6\u5373\u4f7f\u5bb3\u6015\u4e5f\u5fc5\u987b\u5c1d\u8bd5\uff0c\u5c31\u50cf\u83ab\u8389\u867d\u7136\u8df3\u8dc3\u5bf9\u5979\u6765\u8bf4\u5f88\u96be\uff0c\u4ecd\u8981\u9876\u66ff\u52d2\u7f57\u4f0a\u7684\u4f4d\u7f6e\u3002",
  "sec_p7_s0_eli5": "- \u83ab\u8389\u00b7\u970d\u73c0\u5bb3\u6015\u8df3\u3002\n- \u6709\u4e09\u79cd\u8df3\u8dc3\u6e38\u620f\uff1a\u8df3\u9ad8\u3001\u8df3\u8fdc\u548c\u9ebb\u888b\u8d5b\u8dd1\uff08\u4f60\u7ad9\u5728\u4e00\u4e2a\u5927\u888b\u5b50\u91cc\uff0c\u50cf\u5c0f\u5154\u5b50\u4e00\u6837\u8df3\uff09\u3002\n- \u5728\u56fe\u753b\u4e2d\uff0c\u83ab\u8389\u548c\u5979\u7684\u6606\u866b\u670b\u53cb\u4eec\u7a7f\u7740\u84dd\u8272\u886c\u886b\u548c\u7ea2\u8272\u77ed\u88e4\uff0c\u663e\u5f97\u5f88\u62c5\u5fc3\u3002\n- \u5982\u679c\u83ab\u8389\u4e0d\u5c1d\u8bd5\u53bb\u8df3\uff0c\u53e6\u4e00\u652f\u961f\u4f0d\u2014\u2014\u5471\u5471\u4ff1\u4e50\u90e8\u2014\u2014\u53ef\u80fd\u4f1a\u8d62\u5f97\u4e09\u9879\u8df3\u8dc3\u7684\u6240\u6709\u5956\u54c1\u3002\n\u5927\u610f\uff1a\u6709\u65f6\u6211\u4eec\u4f1a\u611f\u5230\u5bb3\u6015\uff0c\u4f46\u5c1d\u8bd5\u53ef\u4ee5\u5e2e\u52a9\u4f60\u7684\u961f\u4f0d\uff0c\u5e76\u9632\u6b62\u5956\u54c1\u88ab\u53e6\u4e00\u5bb6\u4ff1\u4e50\u90e8\u62ff\u8d70\u3002",
  "sec_p8_s0_eli5": "\u60f3\u8c61\u5728\u4e00\u6b21\u8fd0\u52a8\u4f1a\u4e0a\u6709\u4e24\u652f\u9752\u86d9\u961f\u3002\u53bb\u5e74\uff0c\u201c\u5471\u5471\u4ff1\u4e50\u90e8\u201d\u8d62\u5f97\u4e86\u4e09\u4e2a\u5956\u9879\u2014\u2014\u4ed6\u4eec\u62ff\u8d70\u4e86\u6240\u6709\u95ea\u4eae\u7684\u5956\u676f\u3002\u73b0\u5728\uff0c\u5229\u7f57\u4f0a\u3001\u4f26\u5c3c\u548c\u5927\u536b\u544a\u8bc9\u4ed6\u4eec\u7684\u670b\u53cb\u83ab\u8389\u522b\u518d\u5bb3\u6015\uff0c\u8fd9\u6837\u5979\u5c31\u80fd\u5e2e\u52a9\u4ed6\u4eec\u7684\u961f\u4f0d\u3002\u8fd9\u4e00\u6b21\uff0c\u201c\u8df3\u8df3\u4ff1\u4e50\u90e8\u201d\u60f3\u8981\u8d62\u5f97\u8fd9\u4e9b\u5956\u9879\u3002\u5728\u56fe\u7247\u91cc\uff0c\u4f60\u53ef\u4ee5\u770b\u5230\u91d1\u8272\u7684\u5956\u676f\u548c\u770b\u8d77\u6765\u51c6\u5907\u5168\u529b\u4ee5\u8d74\u7684\u9752\u86d9\u4eec\u3002",
  "sec_p9_s0_eli5": "\u628a\u970d\u73c0\u4ff1\u4e50\u90e8\u60f3\u8c61\u6210\u4e00\u652f\u5c0f\u5c0f\u7684\u670b\u53cb\u56e2\u961f\u3002\u4ed6\u4eec\u6b63\u5728\u83b1\u5c3c\u00b7\u970d\u73c0\u5bb6\u5f00\u4f1a\uff0c\u5750\u5728\u5ba2\u5385\u91cc\u3002\n\n\u83b1\u5c3c\u7684\u5988\u5988\u62ce\u7740\u4e00\u4e2a\u888b\u5b50\u63a2\u5934\u8fdb\u6765\u3002\u5979\u8bf4\uff1a\u201c\u5b69\u5b50\u4eec\uff0c\u6211\u53bb\u9694\u58c1\u62ff\u4e9b\u79cd\u5b50\u3002\u201d\u5979\u63d0\u9192\u4ed6\u4eec\uff1a\u7b49\u4f1a\u8bae\u7ed3\u675f\u540e\uff0c\u4f60\u4eec\u53ef\u4ee5\u5230\u5916\u9762\u53bb\u7ec3\u4e60\u2014\u2014\u5c31\u50cf\u4e00\u652f\u961f\u4f0d\u5148\u8ba8\u8bba\uff0c\u7136\u540e\u518d\u7ec3\u4e60\u3002\n\n\u6240\u4ee5\u610f\u601d\u662f\uff1a\u5148\u5728\u5c4b\u91cc\u5f00\u4f1a\uff0c\u7136\u540e\u5230\u5916\u9762\u7ec3\u4e60\uff0c\u800c\u5988\u5988\u5c31\u5728\u9694\u58c1\u62ff\u79cd\u5b50\u3002",
  "sec_p10_s0_eli5": "- \u4f1a\u8bae\u7ed3\u675f\u540e\uff0c\u83ab\u8389\u671b\u5411\u7a97\u5916\uff0c\u53d1\u73b0\u6b63\u5728\u4e0b\u96e8\u3002\n- \u5979\u8bf4\uff1a\u201c\u54e6\uff0c\u4e0d\u201d\uff0c\u56e0\u4e3a\u4e0b\u96e8\u542c\u8d77\u6765\u50cf\u662f\u574f\u6d88\u606f\uff1a\u4ed6\u4eec\u4e0d\u80fd\u5230\u5916\u9762\u53bb\u8bad\u7ec3\u3002\n- \u4f46\u83ab\u8389\u5185\u5fc3\u5176\u5b9e\u5f88\u9ad8\u5174\u3002\u8fd9\u573a\u96e8\u53d6\u6d88\u4e86\u8bad\u7ec3\uff0c\u6240\u4ee5\u5979\u53ef\u4ee5\u5f85\u5728\u5ba4\u5185\u4f11\u606f\u3002\n- \u53ef\u4ee5\u628a\u8fd9\u60f3\u8c61\u6210\uff0c\u5f53\u4f60\u56e0\u4e3a\u5927\u96e8\u610f\u5473\u7740\u5b66\u6821\u6ca1\u6709\u6237\u5916\u6bd4\u8d5b\u800c\u5728\u5fc3\u91cc\u5077\u5077\u4e00\u7b11\u3002\u96e8\u6210\u4e86\u5979\u4e0d\u53bb\u8bad\u7ec3\u7684\u501f\u53e3\u3002"
}